{
    "baseUrl": "/view/js/",
    "paths": {
        "view": "..",
        "view/template": "../ctpl"
    },
    "waitSeconds": 200,
    "shim": {
        "lib/underscore": {
            "exports": "_"
        },
        "lib/jquery": {
            "exports": "$"
        },
        "lib/bootstrap/slider": {
            "deps": ["jquery"],
            "exports": "$.fn.slider"
        },
        "lib/bootstrap/affix": {
            "deps": ["jquery"],
            "exports": "$.fn.affix"
        },
        "lib/bootstrap/alert": {
            "deps": ["jquery"],
            "exports": "$.fn.alert"
        },
        "lib/bootstrap/button": {
            "deps": ["jquery"],
            "exports": "$.fn.button"
        },
        "lib/bootstrap/carousel": {
            "deps": ["jquery"],
            "exports": "$.fn.carousel"
        },
        "lib/bootstrap/collapse": {
            "deps": ["jquery"],
            "exports": "$.fn.collapse"
        },
        "lib/bootstrap/dropdown": {
            "deps": ["jquery"],
            "exports": "$.fn.dropdown"
        },
        "lib/bootstrap/modal": {
            "deps": ["jquery"],
            "exports": "$.fn.modal"
        },
        "lib/bootstrap/popover": {
            "deps": ["jquery"],
            "exports": "$.fn.popover"
        },
        "lib/bootstrap/scrollspy": {
            "deps": ["jquery"],
            "exports": "$.fn.scrollspy"
        },
        "lib/bootstrap/tab": {
            "deps": ["jquery"],
            "exports": "$.fn.tab"
        },
        "lib/bootstrap/tooltip": {
            "deps": ["jquery"],
            "exports": "$.fn.tooltip"
        },
        "lib/bootstrap/transition": {
            "deps": ["jquery"],
            "exports": "$.support.transition"
        },
        "lib/bootstrap/typeahead": {
            "deps": ["jquery"],
            "exports": "$.fn.typeahead"
        }
    },
    "map": {
        "*": {
            "underscore": "lib/underscore",
            "signals": "lib/signals",
            "jquery": "lib/jquery"
        }
    },
    "packages": ["app"]
}