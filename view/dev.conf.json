{
    "baseUrl": "/view/js/",
    "paths": {
        "view": "..",
        "view/template": "../ctpl"
    },
    "waitSeconds": 200,
    "shim": {
        "lib/underscore": {
            "exports": "_"
        },
        "lib/jquery": {
            "exports": "$"
        }
    },
    "map": {
        "*": {
            "underscore": "lib/underscore",
            "signals": "lib/signals",
            "jquery": "lib/jquery"
        }
    },
    "packages": ["app"]
}
